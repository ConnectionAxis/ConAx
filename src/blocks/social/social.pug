mixin social
  .social&attributes(attributes)
    a(href="#").social-item: img.social-icon(src="./images/facebook.png" alt="")
    a(href="#").social-item: img.social-icon(src="./images/twitter.png" alt="")
    a(href="#").social-item: img.social-icon(src="./images/linkedin.png" alt="")
    a(href="#").social-item: img.social-icon(src="./images/gplus.png" alt="")
